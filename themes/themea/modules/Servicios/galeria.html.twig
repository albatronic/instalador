{% set galeria = values.servicio.galeriaFotos %}
{% set nThumbnail = galeria.bloqueThumbnail | length %}

{% if nThumbnail > 0 %}
<div class="ficherosAdjuntos">
    <div class="contenidoTop"></div>
    <div class="contenidoInter">
	<h6 class="verde subrayadoVerde">{{values.LABELS.galeriaDeFotos}}</h6>
	<ul class="galeria">
	    {% for key, galeria in galeria.bloqueThumbnail %}
		<li>
		    <a href="{{app.path}}/{{galeria.PathName}}" rel="gallery" class="pirobox_gall{{values.servicio.objeto.Id}}" title="{{galeria.Title}}"><img src="{{app.path}}/{{galeria.PathName}}" alt="{{galeria.Title}}" title="{{galeria.Title}}" /></a>
		</li>
	    {% endfor %} 
	</ul>
	{% for galeria in galeria.restoImagenes %}
	    <a href="{{app.path}}/{{galeria.PathName}}" rel="gallery" class="pirobox_gall{{values.servicio.objeto.Id}}" title="{{galeria.Title}}"></a>
	{% endfor %}
    </div>
    <div class="contenidoBottom"></div>
</div>
{% endif %}